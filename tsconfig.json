{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app-env.d.ts"
, "app/(dashboard)/[blogId]/(routes)/todo/.jsx", "app/(dashboard)/[blogId]/(routes)/todo/_components/Sidebar.jsx", "app/(dashboard)/[blogId]/(routes)/todo/_components/Sidebar.jsx", "app/(dashboard)/[blogId]/(routes)/todo/_api/boardApi.js"  ],
  "exclude": [
    "node_modules"
  ]
}
